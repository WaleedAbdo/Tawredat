@model TawredatProject.Models.HomePageModel
<!-- Content Header (Page header) -->
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>عرض اشتراكات مديري المشتريات </h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">اشتراكات مديري المشتريات </a></li>
                    <li class="breadcrumb-item active">عرض اشتراكات مديري المشتريات </li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-12">


            <div class="card">
                <div class="card-header">
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                    <table id="example1" class="table table-bordered table-striped ">
                        <thead>
                            <tr>
                                <th>اسم مديري المشتريات </th>
                                <th>اسم الباكاج </th>


                                <th>بداية الاشتراك </th>
                                <th>نهاية الاشتراك </th>
                                <th>تعديل</th>

                                <th>حذف</th>


                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.lstPurchaseSubscribtion)
                            {

                                <tr>
                                    <td>@item.PurchaserName</td>
                                    <td>
                                        @item.SubscriptionPackage
                                    </td>



                                    <td>
                                        @item.StartdDate
                                    </td>
                                    <td>
                                        @item.EnddDate
                                    </td>

                                    <td>
                                        <a asp-area="Admin" asp-controller="PurchaseSubscribtion" asp-action="Form" asp-route-id="@item.PurchaserSubscriptionId" class="btn btn-info text-white" style="cursor:pointer">
                                            تعديل
                                        </a>
                                    </td>
                                    <td>
                                        <a asp-area="Admin" asp-controller="PurchaseSubscribtion" asp-action="Delete" asp-route-id="@item.PurchaserSubscriptionId" class="btn btn-danger text-white" style="cursor:pointer">
                                            حذف
                                        </a>
                                    </td>


                                </tr>

                            }


                        </tbody>
                        <tfoot>
                            <tr>
                                <th>اسم مديري المشتريات </th>
                                <th>اسم الباكاج </th>


                                <th>بداية الاشتراك </th>
                                <th>نهاية الاشتراك </th>
                                <th>تعديل</th>

                                <th>حذف</th>

                            </tr>
                        </tfoot>
                    </table>
                </div>
                <!-- /.card-body -->
            </div>
            <!-- /.card -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
